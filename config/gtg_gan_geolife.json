{
  "name": "GTG-GAN-GEOLIFE",
  "epochs": 1000,
  "batch_size": 256,
  "lr_d": 1e-05,
  "lr_g": 1e-05,
  "beta1": 0.5,
  "beta2": 0.999,
  "opt_g": "adamw",
  "opt_d": "adamw",
  "n_critic": 1,
  "activation": "relu",
  "wgan": false,
  "gradient_penalty": false,
  "lp": false,
  "clip_value": 0.01,
  "lambda_gp": 10,
  "dataset": "geolife",
  "save_freq": 100,
  "plot_freq": 200,
  "latent_dim_g": 256,
  "latent_dim_d": 256,
  "shuffle": true,
  "norm": "layer",
  "conv_only": false,
  "use_stn_in_point_dis": true,
  "use_stn_in_traj_dis": false,
  "sequential_mode": true,
  "generator_lstm": true,
  "discriminator_lstm": false,
  "lstm_latent_dim": 64,
  "bi_lstm": true,
  "bi_lstm_merge_mode": "sum",
  "use_traj_discriminator": true,
  "traj_factor": 1,
  "loss_combination": "sum",
  "share_pointnet": false
}